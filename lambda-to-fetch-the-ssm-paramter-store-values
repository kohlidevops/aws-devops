import json
import boto3

ssm = boto3.client('ssm', region_name='ap-south-1')

def lambda_handler(event, context)
    db_url = ssm.get_parameters(Names=["/my-app/dev/db_url"])
    print(db_url)
    db_password = ssm.get_parameters(Names=["/my-app/dev/db_password"])
    print(db_password)
    return "worked!"
